{
    "compilerOptions": {
      "allowJs": true,
      "checkJs": false,
      "composite": true,
      "downlevelIteration": true,
      "esModuleInterop": true,
      "jsx": "react",
      "lib": [
        "dom",
        "es2017"
      ],
      "module": "commonjs",
      "moduleResolution": "node",
      "preserveConstEnums": true,
      "removeComments": false,
      "resolveJsonModule": true,
      "sourceMap": true,
      "strict": true,
      "target": "es2018"
    }
  }
  